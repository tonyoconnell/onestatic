.breadcrumbs
    margin 0px 0 20px 0  

#breadcrumbs
  padding 10px 0

#breadcrumbs ul
  list-style none
  margin 20px 0  
  
#breadcrumbs li
  display inline-block
  list-style none

.tag-cloud 
  padding-top 20px
  
.tag-cloud ul li 
  float: left
  margin: 4px 4px 4px 0
  height: 32px
  list-style none
  font-size 12px
  padding 0px 
  
.tag-cloud ul li a 
  padding 10px 17px
  color #cacaca
  border-radius 3px
  background #323232
  
.tag-cloud ul li:hover a 
  background #000000
  color #fff