nav
  margin-top -5px
  +tablet
    column(1/2)
  +desktop
    column(1/2)

  ul
    list-style none
    +tablet()
      margin-top 15px
      float right
    +desktop()
      margin-top 15px
      float right  

  li
    display inline-block
    padding 10px 15px
    cursor pointer
    -webkit-transition all 0.2s
    -moz-transition all 0.2s
    -ms-transition all 0.2s
    -o-transition all 0.2s
    transition all 0.2s
    &:hover
      background #111
      color #fff

#breadcrumbs
  padding 10px 0
	ul
	  list-style none
	  margin 10px 0  
  li
	  display inline-block
    
