.contact-bar
  background white
  padding 10px 5px 10px 0px
  

  a
    color black
    letter-spacing 1px
    text-decoration none
    display block
    float left
    padding 2px
    font-size 90%
a
  &:hover
    .circle
      i
        color goldenrod
        
.circle i, icon-text, footer a
  -moz-transition all 0.4s ease-out
  -ms-transition all 0.4s ease-out
  -o-transition all 0.4s ease-out
  transition all 0.4s ease-out

.circle
  i
    float left
    width 28px
    height 28px
    border 0px solid black
    border-radius 100%
    background black
    color white
    margin-right 6px
    font-size 18px
    text-align center
    padding-top 8px
    padding-left 4px
    padding-right 4px


.sign-in
    float right

.heading-1
  color white!important
  background-repeat repeat
  margin 0px!important
  padding 0px
  h1
    color white!important
    margin 0px!important
    padding 10px
  span
    color white!important

.block-title
  padding 5px 10px 10px 10px!important
  margin-bottom 2px
  color white
  font-size 25px
  background url(../pictures/patterns/stressed.png)!important
  background-repeat repeat

.block-content
  background white