.smartmodal-overlay
  display none
  position fixed
  z-index 20000
  width 100%
  height 100%
  top 0
  left 0
  background rgba(0, 0, 0, 0.5)

.smartmodal-modal
  display none
  position fixed
  z-index 99999 !important
  padding 1em
  border-radius .3em
  background white
  color #000
  opacity 1 !important


